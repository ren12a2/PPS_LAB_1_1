//factorial non recursive
#include <stdio.h>
int fact(int);
void main()
{
int n,res;
printf("Factorial using non recursive function\n");
printf("Enter number\n");
scanf("%d", &n);
res=fact(n);
printf("Factorial of %d is %d\n", n, res);
return;
}

int fact(int n){
int i,f=1;
for(i=n;i>0;i--){
    f=f*i;
}
return f;
}
