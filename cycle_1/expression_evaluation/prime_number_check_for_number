#include <stdio.h>

int main() {
    int i,num,flag=0;
    printf("Enter number\n");
    scanf("%d", &num);
    for(i=2;i<=num/2;i++){
        if(num%i==0)
        flag=flag+1;}
        if(flag==0)
        printf("%d is prime\n", num);
        if(flag!=0)
        printf("Not prime\n");
    return 0;
}
